@Library("Infrastructure") _

def product = "sscs"
def component = "sya"

withNightlyPipeline("java", product, component) {

    enablePerformanceTest()

    after('performanceTest') {
        steps.archiveArtifacts allowEmptyArchive: true, artifacts: 'functional-output/**/*'
    }

}